{
  "name": "nextjs-pro-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint": "eslint src --max-warnings=0",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "test": "yarn prettier:check && yarn lint",
    "test:watch": "npm run test -- --watch",
    "generate": "npx plop -- --plopfile generators/plopfile.js",
    "storybook": "start-storybook --no-manager-cache -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "husky install",
    "server": "json-server db.json -w -d 750 -p 3333"
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.3.1",
    "@vercel/og": "^0.0.21",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "connected-next-router": "^4.2.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.2",
    "gsap": "^3.11.4",
    "json-server": "^0.17.0",
    "next": "^13.1.1",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "next-svgr": "^0.0.2",
    "phosphor-react": "^1.4.1",
    "polished": "^4.2.2",
    "rc-slider": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.1",
    "react-icons": "^4.7.1",
    "react-parallax": "^3.5.1",
    "react-scripts": "5.0.1",
    "react-scroll-parallax": "^3.3.2",
    "react-spring": "^9.5.4",
    "styled-components": "5.3.6",
    "swiper": "^8.4.2",
    "three": "^0.148.0",
    "typewriter-effect": "^2.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/styled-components": "^5.1.26",
    "@types/three": "^0.146.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.1.0",
    "plop": "^3.1.1",
    "prettier": "^2.8.1",
    "storybook-addon-next-router": "^4.0.2",
    "typescript": "^4.9.4"
  }
}
